<h1>Sign Up</h1>

<%= form_with url: sessions_path do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <br>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <br>
    <%= f.label :password %>
    <%= f.text_field :password %>
    <br>
    
    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %>
    <br>
    <%= f.submit "submit" %>
<%end%>